{
  "name": "moveable-root",
  "version": "0.30.0",
  "private": true,
  "type": "module",
  "packageManager": "npm@9.0.0",
  "keywords": [
    "moveable",
    "resizable",
    "scalable",
    "draggable",
    "rotatable",
    "groupable",
    "movable",
    "warpable",
    "pinchable",
    "snappable",
    "clippable",
    "roundable",
    "border",
    "origin",
    "dom",
    "resize",
    "scale",
    "drag",
    "move",
    "warp",
    "rotate",
    "react",
    "preact",
    "throttle",
    "group",
    "pinchi",
    "snap"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daybrush/moveable.git"
  },
  "author": "Daybrush",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daybrush/moveable/issues"
  },
  "homepage": "https://daybrush.com/moveable/",
  "scripts": {
    "bootstrap": "npm install",
    "bake": "npm run bake --prefix ./packages/react-moveable",
    "build": "turbo build",
    "build:packages": "turbo build && npm run update-type-consts",
    "lint": "turbo lint",
    "test": "turbo test",
    "test:type": "turbo test:type",
    "coverage": "turbo coverage",
    "dev": "turbo dev",
    "packages:update": "npm run update-type-consts",
    "update-type-consts": "node ./config/update-type-consts.js",
    "packages:build": "npm run build:packages",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "changeset:status": "changeset status",
    "doc": "rm -rf ./doc && jsdoc -c jsdoc.json",
    "demo:build": "npm run packages:build && npm run doc",
    "demo:deploy": "gh-pages -d ./demo --dest=./ --add --remote origin",
    "deploy": "echo 'Deploy script disabled - update as needed'",
    "release": "echo 'Release script disabled - update as needed'"
  },
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@daybrush/jsdoc": "^0.4.7",
    "@daybrush/release": "^0.7.1",
    "@eslint/js": "^9.35.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "cpx": "1.5.0",
    "daybrush-jsdoc-template": "^1.10.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "gh-pages": "^2.0.1",
    "intercept-stdout": "0.1.2",
    "turbo": "^2.5.6",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "overrides": {
    "@types/react": "^19.0.0",
    "typescript": "^5.0.0",
    "@egjs/agent": "^2.2.1",
    "@egjs/children-differ": "^1.0.1",
    "@egjs/list-differ": "^1.0.0",
    "@scena/dragscroll": "^1.4.0",
    "@scena/matrix": "^1.1.1",
    "css-to-mat": "^1.1.1",
    "framework-utils": "^1.1.0",
    "gesto": "^1.19.3",
    "overlap-area": "^1.1.0",
    "croact-css-styled": "^1.1.9",
    "react-css-styled": "^1.1.9",
    "tslib": "^2.3.1"
  }
}
